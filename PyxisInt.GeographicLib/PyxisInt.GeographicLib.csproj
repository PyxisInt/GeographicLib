<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>PyxisInt, LLC.</Authors>
    <Company>PyxisInt, LLC.</Company>
    <Description>PyxisInt.GeographicLib is a C# port of Charles Karney's GeographicLib (https://sourceforge.net/projects/geographiclib/). This product is released under MIT license per the original product.</Description>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/PyxisInt/GeographicLib</PackageProjectUrl>
    <RepositoryUrl>https://github.com/PyxisInt/GeographicLib</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>Copyright 2018, PyxisInt, LLC. and Charles F. F. Karney</Copyright>
    <Version>2.0.0</Version>
    <LangVersion>latestmajor</LangVersion>
  </PropertyGroup>
  <Target Name="UpdateVersion">
    <PropertyGroup>
      <Version>2.0.$(VersionSuffix)</Version>
    </PropertyGroup>
  </Target>

</Project>
